{
  "name": "@datewise/api",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run clean && npm run build --workspace @datewise/shared && nest start --watch",
    "build": "npm run clean && nest build",
    "start": "node dist/main.js",
    "typecheck": "npm run build --workspace @datewise/shared && tsc --noEmit",
    "test": "npm run build && node --test dist/**/*.spec.js",
    "clean": "node -e \"const fs=require('node:fs'); for (const p of ['dist','tsconfig.tsbuildinfo','tsconfig.build.tsbuildinfo']) fs.rmSync(p,{ recursive: true, force: true });\""
  },
  "dependencies": {
    "@datewise/shared": "0.1.0",
    "@nestjs/common": "^10.4.9",
    "@nestjs/core": "^10.4.9",
    "@nestjs/platform-express": "^10.4.9",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.8",
    "@types/node": "^22.9.0",
    "typescript": "^5.6.3"
  }
}
